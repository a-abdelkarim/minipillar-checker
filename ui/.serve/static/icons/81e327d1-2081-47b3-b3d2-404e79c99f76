import math


def CalcEarthRaduis(latitude):
    R_Equator = 6378.137
    R_Pole = 6356.752
    A = math.pow((R_Equator * R_Equator * math.cos(latitude)), 2.0)
    B = math.pow((R_Pole * R_Pole * math.sin(latitude)), 2.0)
    C = math.pow((R_Equator * math.cos(latitude)), 2.0)
    D = math.pow((R_Pole * math.sin(latitude)), 2.0)
    return (math.sqrt((A + B) / (C + D)))


CalcEarthRaduis(33)
